  m_Name: DialogueData
  m_EditorClassIdentifier: 
  speakers:
  - {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
  textAssets:
  - {fileID: 4900000, guid: 55fe6de456d1ad246b7c1e5c9505894c, type: 3}
  dialogues:
  - name: Tutorial
    flags: 0
    elems: []
    conditionalNext:
    - condition: TutorialStage == 0
      nextKey:
        nextKey: IntroText
        code: []
    - condition: TutorialStage == 1
      nextKey:
        nextKey: PresentSeed
        code: []
  - name: IntroText
    flags: 1
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Welcome, Unlighted One!
      options: []
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: I'm so happy you're here! Now, we can bring light back!
      options: []
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Darkness is very dangerous! Walk in the dark for a bit and you'll see!
      options: []
    conditionalNext: []
  - name: Death1
    flags: 0
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: 'See?!

        Dangerous!!'
      options: []
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: But don't worry, you will always come back, although something may be
        lost in the process!
      options: []
    conditionalNext:
    - condition: 
      nextKey:
        nextKey: Death1:Question
        code: []
  - name: Death1:Question
    flags: 0
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Are you ready to go on?
      options:
      - text: No, I'm scared!
        key: Death1:No
      - text: Yes, let's go!
        key: Death1:Yes
    conditionalNext: []
  - name: Death1:No
    flags: 0
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Don't be, as an Unlighted you always come back, even if it is unpleasant!
      options: []
    conditionalNext:
    - condition: 
      nextKey:
        nextKey: Death1:Question
        code: []
  - name: Death1:Yes
    flags: 0
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Yay! Very well! Follow the light and find the exit, I'll meet you in
        the other side!
      options: []
    conditionalNext:
    - condition: 
      nextKey:
        nextKey: 
        code:
        - isFunctionCall: 0
          functionOrVarName: TutorialStage
          expressions:
          - 1
        - isFunctionCall: 1
          functionOrVarName: Teleport
          expressions:
          - '"Crystal"'
          - '"CrystalTeleportLocation1"'
        - isFunctionCall: 1
          functionOrVarName: Close
          expressions: []
  - name: PresentSeed
    flags: 0
    elems:
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: You're here! I knew you could do it!
      options: []
    - speaker: {fileID: 11400000, guid: e92f9206af924614d878c749db6e61e4, type: 2}
      text: Now, grab that shovel over there, we're going to do something really
        neat!
      options: []
    conditionalNext:
    - condition: 
      nextKey:
        nextKey: 
        code:
        - isFunctionCall: 1
          functionOrVarName: SpawnShovel
          expressions: []
        - isFunctionCall: 1
          functionOrVarName: Close
          expressions: []
